<odoo>
    <data>
        <record model="ir.actions.act_window" id="modulos_list_action">
            <field name="name">Modulos</field>
            <field name="res_model">odoo_pruebas_relaciones.modulos</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">No hay m√≥dulos</p>
            </field>
        </record>

        <record model="ir.ui.view" id="modulo_tree_view">

            <field name="name">modulos.tree</field>
            <field name="model">odoo_pruebas_relaciones.modulos</field>
            <field name="arch" type="xml">

                <tree string="Modulo">
                    <field name="name"/>
                </tree>

            </field>
        </record>
    </data>
</odoo>